{
  "name": "midu-theme",
  "version": "1.0.0",
  "description": "WordPress theme with Sass compilation",
  "scripts": {
    "sass": "sass assets/scss/main.scss assets/scss/main.css --style expanded --source-map",
    "sass:watch": "sass --watch assets/scss/main.scss:assets/scss/main.css --style expanded --source-map",
    "sass:compressed": "sass assets/scss/main.scss assets/scss/main.css --style compressed --source-map"
  },
  "devDependencies": {
    "sass": "^1.77.0"
  },
  "keywords": [
    "wordpress",
    "theme",
    "sass"
  ],
  "author": "",
  "license": "ISC"
}
